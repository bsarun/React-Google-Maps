    "build-with-devtools": "webpack --progress -p --colors --define process.env.devtoolsFooter='\"true\"' --config ./webpack.production.config.js",
    "build-qa": "webpack --progress -p --colors --config ./webpack.qa.config.js",
    "build-vpn": "webpack --progress -p --colors --config ./webpack.vpn.config.js",
    "build-vpn-with-devtools": "webpack --progress -p --colors --define process.env.devtoolsFooter='\"true\"' --config ./webpack.vpn.config.js",
